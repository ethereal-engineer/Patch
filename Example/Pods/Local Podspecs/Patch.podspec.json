    },
    {
      "name": "iCarousel",
      "dependencies": {
        "Patch/base": [
          "iCarousel"
        ]
      },
      "source_files": "Pod/Classes/Adapters/PDSCarouselDataSourceAdapter.{m,h}"
    },
    {
      "name": "CoreData",
      "dependencies": {
        "Patch/base": [

        ]
      },
      "frameworks": "CoreData",
      "source_files": "Pod/Classes/Extensions/PDSCoreDataSource.{m,h}"
    }
  ]
}
